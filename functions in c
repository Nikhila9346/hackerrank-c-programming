#include <iostream>
#include <cstdio>
using namespace std;

/*
Add `int max_of_four(int a, int b, int c, int d)` here.
*/
int max_of_four(int a,int b,int c,int d);
int main() {
    int a, b, c, d;
    scanf("%d %d %d %d", &a, &b, &c, &d);
    int ans =max_of_four(a, b, c, d);
    printf("%d", ans);
}
    int max_of_four(int a,int b,int c,int d)
{
    int ans;
    if(a>b)
    ans=a;
    else
    ans=b;
    if(ans>c)
    ans=ans;
    else
    ans=c;
    if(ans>d)
    ans=ans;
    else
    ans=d;
    return ans;
}
