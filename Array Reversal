#include <stdio.h>
#include <string.h>
#include <stdlib.h>

int main()
{
  int n,i,j,temp;
  scanf("%d",&n);
  int arr[n];
  for(int k=0;k<n;k++)
scanf("%d",&arr[k]);
  for(i=0,j=n-1;i<j;i++,j--)
  {
      temp=arr[i];
      arr[i]=arr[j];
      arr[j]=temp;
  }
  for(int l=0;l<n;l++)
  printf("%d ",arr[l]);
}  
